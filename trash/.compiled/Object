__Object__isKindOf_() {
  local className="$1"
    local current
    current=$(@ Runtime class)
    while [[ -n "$current" ]]; do
        if [[ "$current" == "$1" ]]; then
            echo "true"
            return 0
        fi
        current=$(@ Runtime superclassOf: "$current")
    done
    echo "false"
}

