__Object__inspect() {
    local data class instance
    class=$(@ Runtime class)
    instance=$(@ Runtime instance)
    data=$(@ Runtime dataFor: "$_RECEIVER")

    @ Console print: "a $class"
    @ Console print: " id: $instance"

    if [[ -n "$data" && "$data" != "{}" ]]; then
        echo "$data" | jq -r 'to_entries | .[] | " \(.key): \(.value)"'
        else
        @ Trash methodsFor "$_RECEIVER"
    fi
}

