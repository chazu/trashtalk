__Console__class__print_color_() {
  local message="$1"
  local color="$2"
    local code
    case "$2" in
        black) code="30";;
        red) code="31";;
        green) code="32";;
        yellow) code="33";;
        blue) code="34";;
        magenta) code="35";;
        cyan) code="36";;
        white) code="37";;
        *) code="0";;
    esac
    printf "\033[${code}m%s\033[0m\n" "$1"
}

