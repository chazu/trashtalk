  local filepath="$2"
  eval "$1" > "$2"
}

__Shell__class__exec_appendTo_() {
  local command="$1"
  local filepath="$2"
  eval "$1" >> "$2"
}

__Shell__class__if_then_() {
  local condition="$1"
  local command="$2"
  if eval "$1" > /dev/null 2 >& 1 ; then
  eval "$2"
  fi
}

__Shell__class__unless_then_() {
  local condition="$1"
  local command="$2"
  if ! eval "$1" > /dev/null 2 >& 1 ; then
  eval "$2"
  fi
}

