__Shell__class__kill_() {
  local pid="$1"
  kill - KILL "$1" 2 > /dev/null
}

__Shell__class__pause_() {
  local pid="$1"
  kill - STOP "$1" 2 > /dev/null
}

__Shell__class__resume_() {
  local pid="$1"
  kill - CONT "$1" 2 > /dev/null
}

