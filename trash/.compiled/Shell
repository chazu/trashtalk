  local command="$1"
  local pipeCommand="$2"
  eval "$1" | eval "$2"
}

__Shell__class__exec_pipeTo_pipeTo_() {
  local command="$1"
  local pipe1="$2"
  local pipe2="$3"
  eval "$1" | eval "$2" | eval "$3"
}

