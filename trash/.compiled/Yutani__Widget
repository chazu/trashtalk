    session="$(_ivar session)"
    wid="$(_ivar widgetId)"
    dispatcher="$(_ivar dispatcher)"
    # Remove handler from dispatcher
    if [[ -n "$dispatcher" && -n "$wid" ]]; then
      @ "$dispatcher" removeHandlerFor: "$wid"
    fi
    # Delete from server
    if [[ -n "$session" && -n "$wid" ]]; then
      @ "$session" deleteWidget: "$wid"
    fi
  ]

  # Focus this widget
