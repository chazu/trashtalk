    _create_instance File "$id"
    @ "$id" setPath "$filepath"
    echo "$id"
  ]

  # Create a temporary file with optional prefix
  # Usage: file=$(@ File temp)
  # Usage: file=$(@ File tempWithPrefix: "st-inspect-")
  rawClassMethod: temp [
    local tmpfile
    tmpfile=$(mktemp)
    local id=$(_generate_instance_id File)
    _create_instance File "$id"
    @ "$id" setPath "$tmpfile"
    echo "$id"
  ]

