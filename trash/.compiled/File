    local tmpfile
    tmpfile=$(mktemp -t "${prefix}XXXXXX")
    local id=$(_generate_instance_id File)
    _create_instance File "$id"
    @ "$id" setPath "$tmpfile"
    echo "$id"
  ]

  # Create a named pipe (FIFO)
  # Usage: fifo=$(@ File mkfifo: "/tmp/myfifo")
  rawClassMethod: mkfifo: filepath [
    mkfifo "$filepath" 2>/dev/null
    local id=$(_generate_instance_id File)
    _create_instance File "$id"
    @ "$id" setPath "$filepath"
    echo "$id"
  ]

  # Get the file path
  rawMethod: path [
    _ivar path
  ]

  # Check if file exists
  method: exists [
    (path fileExists) ifTrue: [^ 'true'] ifFalse: [^ 'false']
  ]

  # Check if it's a regular file
  method: isFile [
    (path isFile) ifTrue: [^ 'true'] ifFalse: [^ 'false']
  ]

  # Check if it's a directory
  method: isDirectory [
    (path isDirectory) ifTrue: [^ 'true'] ifFalse: [^ 'false']
