    len=$(echo "$items" | jq 'length')
    acc="$1"

    for ((i=0; i<len; i++)); do
        element=$(echo "$items" | jq -r ".[$i]")
        acc=$(@ "$block_id" valueWith_and "$acc" "$element")
    done

