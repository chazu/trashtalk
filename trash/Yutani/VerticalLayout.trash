package: Yutani

VerticalLayout subclass: Yutani::FlexLayout

  # Create a new VerticalLayout
  classMethod: inSession: sess [
    | instance wid |
    instance := @ Yutani::VerticalLayout new.
    @ instance setSession: sess.
    @ instance setDirection: 'FLEX_ROW'.
    wid := @ sess createVerticalLayout.
    @ instance setWidgetId: wid.
    ^ instance
  ]
