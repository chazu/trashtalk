package: Yutani

HorizontalLayout subclass: Yutani::FlexLayout

  # Create a new HorizontalLayout
  classMethod: inSession: session [
    | instance wid |
    instance := $(@ Yutani::HorizontalLayout new).
    @ instance setSession: session.
    @ instance setDirection: 'FLEX_COLUMN'.
    wid := $(@ session createHorizontalLayout).
    @ instance setWidgetId: wid.
    ^ instance
  ]
